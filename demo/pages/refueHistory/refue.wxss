  font-size: 12px;
  padding-left: 15px;
  box-sizing: border-box;
}
.detail .log-item{
  display: inline-block;
}
.grayColor{
  color: #848484;
}
.detailList{
  padding: 10px 10px 0 10px;
  box-sizing: border-box;
  font-size: 12px;
}
.detailEveryOne{
  display: block;
  background-color: #fff;
  border-radius: 5px;
  box-sizing: border-box;
  padding: 5px 5px 5px 5px;
  position: relative;
  margin-bottom: 15rpx;
}
.detailTitle{
  border-bottom: 1px dashed #e7e7e7;
  height: 35px;
  line-height: 35px;
  padding-left: 5px;
}
.oilImg{
    display: inline-block;
    width: 45rpx;
    height: 45rpx;
    background-size: 45rpx 45rpx;
    vertical-align: middle;
}
.datailCon{
  background-color: #fff;
  color: #848484;
  padding: 10px 35px 10px 35px;
}
.datailCon>view{
  line-height: 25px;
}
.conLeft{
  width: 80px;
  display: inline-block;
}
.conRight{
  float: right;
  text-align: right;
}
.fixCustomerLogo{
  position: absolute;
  top: 0;
  right: 30rpx;
  width: 90rpx;
  height: 70rpx;
  background-size: 90rpx 70rpx;
}
.loadingWen{
  text-align: center;
  color: #cccccc;
  display: block;
  padding: 10rpx 0;
}
.spinner {
  margin: 0 auto;
  width: 150px;
  text-align: center;
}

.spinner > div {
  width: 30rpx;
  height: 30rpx;
  background-color: #cccccc;

  border-radius: 100%;
  display: inline-block;
  -webkit-animation: bouncedelay 1.4s infinite ease-in-out;
  animation: bouncedelay 1.4s infinite ease-in-out;
  /* Prevent first frame from flickering when animation starts */
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  vertical-align: middle;
}

.spinner .bounce1 {
  -webkit-animation-delay: -0.32s;
  animation-delay: -0.32s;
}

.spinner .bounce2 {
  -webkit-animation-delay: -0.16s;
  animation-delay: -0.16s;
}

@-webkit-keyframes bouncedelay {
  0%, 80%, 100% { -webkit-transform: scale(0.1) }
  40% { -webkit-transform: scale(0.5) }
}

@keyframes bouncedelay {
  0%, 80%, 100% {
    transform: scale(0.1);
    -webkit-transform: scale(0.1);
  } 40% {
      transform: scale(0.5);
      -webkit-transform: scale(0.5);
    }
}